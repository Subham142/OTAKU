{
    "builds":[{
        "src":"./backend/server.js",
        "use":"@vercel/node"
    }],
    "routes":[
        {
            "src": "/api/(.*)",
            "dest": "/backend/server.js"
        }
    ],
    
        "rewrites": [
            {
                 "source": "/(.*)", 
                 "destination": "/api" 
         }
                ]
             
} 